<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.domino.t1.address.AddressDAO">

	<select id="getMemberAddress" parameterType="AddressDTO" resultType="AddressDTO">
		select * from member_address where member_num = #{member_num} order by member_num desc
	</select>

	<insert id="setMemberAddress" parameterType="AddressDTO">
		insert into member_address values(#{member_num}, #{roadFullAddr})
	</insert>

</mapper>